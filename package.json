{
  "name": "safe-analysis",
  "version": "1.0.0",
  "description": "Analysis of top protocols Safe wallets interact with on mainnet",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "analyze": "npm run build && node dist/analyze.js",
    "dune": "npm run build && node dist/dune-query.js",
    "protocols": "npm run build && node dist/protocol-analysis.js",
    "clean": "rimraf dist",
    "watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "viem": "^2.21.4",
    "ethers": "^6.13.2",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "nodemon": "^3.1.4"
  },
  "keywords": ["safe", "ethereum", "defi", "protocols", "analytics", "typescript"],
  "author": "",
  "license": "MIT"
}